{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/uploads/(.*)",
            "dest": "/public/uploads/$1"
        },
        {
            "src": "/api/(.*)",
            "dest": "index.js"
        }
    ],
    "static": {
        "public": "public"
    }
}